{
    "name": "darren-nolan/styler",
    "description": "CSS Styler package for Laravel. Compiles Less/SCSS on the fly and supports serving cached content.",
    "keywords": ["laravel", "scss", "less", "css", "stylesheets", "styles"],
    "license": "MIT",
    "authors": [
        {
            "name": "Darren Nolan",
            "email": "me@darrennolan.com",
            "homepage": "http://darrennolan.com"
        }
    ],
    "require": {
        "php":                ">=5.3.0",
        "illuminate/support": ">=4.0.x",
        "illuminate/http":    ">=4.0.x",
        "leafo/scssphp":      "0.0.9",
        "leafo/lessphp":      "0.4.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "DarrenNolan\\Styler": "src/"
        }
    },
    "minimum-stability": "dev"
}
